{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/Samyak_Updated_CV.pdf",
      "headers": {
        "cache-control": "max-age=31536000"
      },
      "dest": "/Samyak_Updated_CV.pdf"
    },
    {
      "src": "/Resume/(.*)",
      "headers": {
        "cache-control": "max-age=31536000"
      },
      "dest": "/Resume/$1"
    },
    {
      "src": "/images/(.*)",
      "headers": {
        "cache-control": "max-age=31536000"
      },
      "dest": "/images/$1"
    },
    {
      "src": "/assets/(.*)",
      "headers": {
        "cache-control": "max-age=31536000"
      },
      "dest": "/assets/$1"
    },
    {
      "src": "/favicon-(.*)",
      "headers": {
        "cache-control": "max-age=31536000"
      },
      "dest": "/favicon-$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}